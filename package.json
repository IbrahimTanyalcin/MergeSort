{
  "name": "ibowankenobi-mergesort",
  "version": "0.0.1",
  "description": "Merge Sort algorithm implementation without recursion, using cached binary trees",
  "main": "./dist/mergesort.0.0.1.evergreen.min.js",
  "access": "public",
  "scripts": {
    "test": "cross-env-shell MERGESORT_VERSION=${npm_package_version} jest --ci",
    "prependModuleDef": "cross-env-shell CUSTOM_LOC=./dist node ./util/prependModuleDef.js",
    "buildEvergreen": "cross-env-shell MERGESORT_VERSION=${npm_package_version} MERGESORT_GLOBAL_NAME=${npm_package_name} rollup -c ./rollup.config.evergreen.js",
    "buildEs5": "cross-env-shell MERGESORT_VERSION=${npm_package_version} MERGESORT_GLOBAL_NAME=${npm_package_name} rollup -c ./rollup.config.es5.js",
    "minEvergreen": "cross-env-shell uglifyjs ./dist/mergesort.${npm_package_version}.evergreen.js -o ./dist/mergesort.${npm_package_version}.evergreen.min.js",
    "minEs5": "cross-env-shell uglifyjs ./dist/mergesort.${npm_package_version}.es5.js -o ./dist/mergesort.${npm_package_version}.es5.min.js",
    "build": "cross-env-shell npm run buildEvergreen && npm run buildEs5 && npm run minEvergreen && npm run minEs5 && npm run prependModuleDef && npm run jsdoc",
    "jsdoc": "cross-env-shell ./node_modules/.bin/jsdoc -c ./jsdoc.conf -R ./README.md ./dist/",
    "updatePckgJSON": "cross-env-shell CUSTOM_LOC=./ node ./util/updatePckgJSON.js",
    "backUpFiles": "cross-env-shell CUSTOM_LOC=./dist/ CUSTOM_DEST=./history node ./util/backUpFiles.js",
    "cleanUp": "cross-env-shell CUSTOM_LOC=./dist/ node ./util/cleanUp.js",
    "bumpPatch": "cross-env-shell npm version patch -no-git-tag-version --force",
    "bumpMinor": "cross-env-shell npm version minor -no-git-tag-version --force",
    "bumpMajor": "cross-env-shell npm version major -no-git-tag-version --force",
    "gitCommit": "cross-env-shell git commit -m ${npm_package_version}",
    "gitTag": "cross-env-shell git tag ${npm_package_version}",
    "gitAddAll": "git add -A",
    "gitPush": "git push",
    "gitPushTags": "git push --tags",
    "setGit": "npm run gitAddAll && npm run gitCommit && npm run gitTag && npm run gitPush && npm run gitPushTags",
    "publishRest": "npm run backUpFiles && npm run cleanUp && npm run build && npm run updatePckgJSON && npm run setGit && npm publish",
    "publishPatch": "npm run bumpPatch && npm run publishRest",
    "publishMinor": "npm run bumpMinor && npm run publishRest",
    "publishMajor": "npm run bumpMajor && npm run publishRest"
  },
  "keywords": [
    "sort",
    "stable",
    "javascript",
    "algorithm",
    "performance",
    "binary",
    "tree",
    "merge",
    "mergesort",
    "timsort",
    "insertionsort"
  ],
  "author": "Ibrahim Tanyalcin",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.14.8",
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "@babel/runtime": "^7.14.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-es5": "^1.2.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.6",
    "js-beautify": "^1.14.0",
    "jsdoc": "^3.6.7",
    "puppeteer": "^10.2.0",
    "rollup": "^2.56.0",
    "uglify-js": "^3.14.1"
  },
  "jest": {
    "globalSetup": "./src/globalSetup.js"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.14.9",
    "core-js": "^3.16.0"
  }
}